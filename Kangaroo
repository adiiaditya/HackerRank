#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int x1; 
    int v1; 
    int x2; 
    int v2; 
    scanf("%d %d %d %d",&x1,&v1,&x2,&v2);
    
    if(0<=x1<x2<=10000 && 1<=v1<=10000 && 1<=v2<=10000)
        {
         if(x2>x1 && v2>=v1)
        {
        printf("NO");
    }
        else
            {
                if((x1-x2) % (v2-v1) == 0)
                    {
                    printf("YES");
                }
                else
                    {
                    printf("NO");
                }
            
            }
   
    
    return 0;
    }
}
